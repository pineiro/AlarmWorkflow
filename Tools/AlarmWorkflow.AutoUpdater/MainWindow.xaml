        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:AlarmWorkflow.Tools.AutoUpdater.Views"
        Title="AlarmWorkflow AutoUpdater" Height="480" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top">

        </Menu>

        <TabControl TabStripPlacement="Bottom">
            <TabItem Header="Paketliste">
                <views:PackageListControl Margin="1" />
            </TabItem>
            <TabItem Header="Log">
                <views:LogControl Margin="1" />
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
        
    